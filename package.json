{
  "name": "mern-ts",
  "repository": "https://github.com/tyty5949/MERN-TypeScript",
  "license": "ISC",
  "scripts": {
    "db:start": "brew services start mongodb-community@4.2",
    "db:start-windows": "\"C:\\Program Files\\MongoDB\\Server\\4.2\\mongod\"",
    "db:stop": "brew services stop mongodb-community@4.2",
    "db:seed": "tsc && node build/database/seed.js",
    "migrate:up": "tsc && node build/database/migrate_up.js",
    "migrate:down": "tsc && node build/database/migrate_down.js",
    "prebuild": "rimraf dist build",
    "build": "npm run-script prebuild && nest build app && webpack -p",
    "prettier": "prettier --write \"server/**/*.ts\" \"client/**/*.{ts,tsx}\"",
    "start:dev": "concurrently \"webpack-dev-server --no-inline\" \"nest start --watch\"",
    "start:prod": "node build/main",
    "lint": "eslint '{server,client}/**/*.ts'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "echo 'No e2e tests implemented yet.'",
    "start": "npm run-script start:prod"
  },
  "dependencies": {
    "@nestjs/common": "^7.2.0",
    "@nestjs/core": "^7.2.0",
    "@nestjs/platform-express": "^7.2.0",
    "@sentry/node": "^5.17.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "connect-mongo": "^3.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "express-winston": "^4.0.3",
    "hbs": "^4.1.1",
    "mongodb": "^3.5.9",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.5",
    "typescript": "^3.9.5",
    "winston": "^3.2.1",
    "winston-transport": "^4.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@types/express": "^4.17.6",
    "@types/jest": "^26.0.3",
    "@types/mongodb": "^3.5.20",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.2.0",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.2.0",
    "copy-webpack-plugin": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.1.0",
    "prettier": "2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "source-map-loader": "^1.0.1",
    "ts-jest": "^26.1.1",
    "ts-loader": "^7.0.5",
    "ts-node-dev": "^1.0.0-pre.44",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
